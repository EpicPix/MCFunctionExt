[
  {
    "name": "advancement",
    "syntax": {
      "grant|revoke": {
        "@selector": {
          "everyone": "@end",
          "only": {
            "@advancement": {
              "@criterion": "@end"
            }
          },
          "from|through|until": {
            "@advancement": "@end"
          }
        }
      }
    }
  },
  {
    "name": "bossbar",
    "syntax": {
      "add|remove": {
        "@word": "@any"
      },
      "get": {
        "max|players|value|visible": "@end"
      },
      "list": "@end",
      "set": {
        "@word": {
          "color": {
            "blue|green|pink|purple|red|white|yellow": "@end"
          },
          "max": {
            "@int": "@end"
          },
          "name": "@any",
          "players": {
            "@selector": "@end"
          },
          "style": {
            "notched_6|notched_10|notched_12|notched_20|progress": "@end"
          },
          "@value": {
            "@int": "@end"
          },
          "visible": {
            "@boolean": "@end"
          }
        }
      }
    }
  },
  {
    "name": "data",
    "syntax": {
      "get": {
        "@multi block @vec3d/entity @selector/storage @resource": {
          "@none": "@end",
          "@nbt_path": {
            "@none": "@end",
            "@double": "@end"
          }
        }
      },
      "merge": {
        "@multi block @vec3d/entity @selector/storage @resource": "@any"
      },
      "modify": {
        "@multi block @vec3d/entity @selector/storage @resource": {
          "@nbt_path": {
            "@multi append/insert @int/merge/prepend/set": {
              "from": {
                "@multi block @vec3d/entity @selector/storage @resource": {
                  "@nbt_path": "@end"
                }
              },
              "value": "@any"
            }
          }
        }
      },
      "remove": {
        "@multi block @vec3d/entity @selector/storage @resource": "@any"
      }
    }
  },
  {
    "name": "defaultgamemode",
    "syntax": {
      "survival|creative|adventure|spectator": "@end"
    }
  },
  {
    "name": "difficulty",
    "syntax": {
      "peaceful|easy|normal|hard": "@end"
    }
  },
  {
    "name": "execute",
    "syntax": {
      "anchored": {
        "eyes|feet": "@command execute"
      },
      "as|at": {
        "@selector": "@command execute"
      },
      "facing": {
        "entity": {
          "@selector": {
            "eyes|feet": "@command execute"
          }
        },
        "@vec3d": "@command execute"
      },
      "positioned": {
        "as": {
          "@selector": "@command execute"
        },
        "@vec3d": "@command execute"
      },
      "run": "@command"
    }
  },
  {
    "name": ["experience", "xp"],
    "syntax": {
      "add|set": {
        "@selector": {
          "@int": {
            "@has": {
              "levels|points": "@end"
            },
            "@none": "@end"
          }
        }
      },
      "query": {
        "@selector": {
          "levels|points": "@end"
        }
      }
    }
  },
  {
    "name": "forceload",
    "syntax": {
      "add": {
        "@vec2d": {
          "@none": "@end",
          "@vec2d": "@end"
        }
      },
      "remove": {
        "all": "@end",
        "@vec2d": {
          "@none": "@end",
          "@vec2d": "@end"
        }
      },
      "query": {
        "@vec2d": "@end"
      }
    }
  },
  {
    "name": "function",
    "syntax": {
      "@function": "@end"
    }
  },
  {
    "name": "gamemode",
    "syntax": {
      "survival|creative|adventure|spectator": {
        "@has": {
          "@selector": "@end"
        },
        "@none": "@end"
      }
    }
  },
  {
    "name": "kill",
    "syntax": {
      "@has": {
        "@selector": "@end"
      },
      "@none": "@end"
    }
  },
  {
    "name": "me",
    "syntax": "@any"
  },
  {
    "name": ["msg", "tell", "w"],
    "syntax": {
      "@selector": "@any"
    }
  },
  {
    "name": "say",
    "syntax": "@any"
  },
  {
    "name": "seed",
    "syntax": "@end"
  },
  {
    "name": "schedule",
    "syntax": {
      "function": {
        "@function": {
          "@time": {
            "append|replace": "@end",
            "@none": "@end"
          }
        }
      },
      "clear": {
        "@function": "@end"
      }
    }
  },
  {
    "name": "tag",
    "syntax": {
      "@selector": {
        "add|remove": {
          "@word": "@end"
        },
        "list": "@end"
      }
    }
  },
  {
    "name": "tellraw",
    "syntax": {
      "@selector": {
        "@json": "@end"
      }
    }
  },
  {
    "name": "time",
    "syntax": {
      "add": {
        "@time": "@end"
      },
      "query": {
        "daytime|gametime|day": "@end"
      },
      "set": {
        "day|night|noon|midnight": "@end",
        "@time": "@end"
      }
    }
  },
  {
    "name": "trigger",
    "syntax": {
      "@word": {
        "add|set": {
          "@int": "@end"
        },
        "@none": "@end"
      }
    }
  },
  {
    "name": "weather",
    "syntax": {
      "clear|rain|thunder": {
        "@has": {
          "@int": "@end"
        },
        "@none": "@end"
      }
    }
  },
  {
    "name": "method",
    "vanilla": false
  }
]