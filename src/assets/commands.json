[
  {
    "name": "advancement",
    "syntax": {
      "grant|revoke": {
        "@selector": {
          "everyone": "@end",
          "only": {
            "@advancement": {
              "@criterion": "@end"
            }
          },
          "from|through|until": {
            "@advancement": "@end"
          }
        }
      }
    }
  },
  {
    "name": "bossbar",
    "syntax": {
      "add|remove": {
        "@word": "@any"
      },
      "get": {
        "max|players|value|visible": "@end"
      },
      "list": "@end",
      "set": {
        "@word": {
          "color": {
            "blue|green|pink|purple|red|white|yellow": "@end"
          },
          "max": {
            "@int": "@end"
          },
          "name": "@any",
          "players": {
            "@selector": "@end"
          },
          "style": {
            "notched_6|notched_10|notched_12|notched_20|progress": "@end"
          },
          "@value": {
            "@int": "@end"
          },
          "visible": {
            "@boolean": "@end"
          }
        }
      }
    }
  },
  {
    "name": "defaultgamemode",
    "syntax": {
      "survival|creative|adventure|spectator": "@end"
    }
  },
  {
    "name": "difficulty",
    "syntax": {
      "peaceful|easy|normal|hard": "@end"
    }
  },
  {
    "name": "execute",
    "syntax": {
      "facing": {
        "entity": {
          "@selector": {
            "eyes|feet": "@command execute"
          }
        },
        "@vec3d": "@command execute"
      },
      "run": "@command"
    }
  },
  {
    "name": ["experience", "xp"],
    "syntax": {
      "add|set": {
        "@selector": {
          "@int": {
            "@has": {
              "levels|points": "@end"
            },
            "@none": "@end"
          }
        }
      },
      "query": {
        "@selector": {
          "levels|points": "@end"
        }
      }
    }
  },
  {
    "name": "function",
    "syntax": {
      "@function": "@end"
    }
  },
  {
    "name": "gamemode",
    "syntax": {
      "survival|creative|adventure|spectator": {
        "@has": {
          "@selector": "@end"
        },
        "@none": "@end"
      }
    }
  },
  {
    "name": "kill",
    "syntax": {
      "@has": {
        "@selector": "@end"
      },
      "@none": "@end"
    }
  },
  {
    "name": "me",
    "syntax": "@any"
  },
  {
    "name": ["msg", "tell", "w"],
    "syntax": {
      "@selector": "@any"
    }
  },
  {
    "name": "say",
    "syntax": "@any"
  },
  {
    "name": "seed",
    "syntax": "@end"
  },
  {
    "name": "tag",
    "syntax": {
      "@selector": {
        "add|remove": {
          "@word": "@end"
        },
        "list": "@end"
      }
    }
  },
  {
    "name": "trigger",
    "syntax": {
      "@word": {
        "add|set": {
          "@int": "@end"
        },
        "@none": "@end"
      }
    }
  },
  {
    "name": "weather",
    "syntax": {
      "clear|rain|thunder": {
        "@has": {
          "@int": "@end"
        },
        "@none": "@end"
      }
    }
  }
]